<script>

  $(function () {
    $("#example1").DataTable({
      "responsive": true, "lengthChange": false, "autoWidth": false,
      "buttons": ["excel", "pdf", "print", "colvis"]
    }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
    $('#example2').DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false,
      "responsive": true,
    });

    $('.datemask').mask('00/00/0000');
    $('.cpf').mask('000.000.000-00');

    $('.salary').mask('0000000000000.00', {
      reverse: true
    });


    $('.placa').mask('ZZZZZZZ', {
      translation: {
        'Z': {
          pattern: /[0-9a-zA-Z]/,
          optional: true
        }
      }
    });

  });

  window.onload = modal;

  function modal() {
    //Array de parametros 'chave=valor'
    var params = window.location.search.substring(1).split('&');

    //Criar objeto que vai conter os parametros
    var paramArray = {};

    //Passar por todos os parametros
    for (var i = 0; i < params.length; i++) {
      //Dividir os parametros chave e valor
      var param = params[i].split('=');

      //Adicionar ao objeto criado antes
      paramArray[param[0]] = param[1];
    }
    if (paramArray['v']) {
      $("#modal-default").modal({
        show: true
      });

      document.getElementById("formUpload").action = "/upload?v=" + paramArray['v']
    }

  }
  function messegeDelete(file) {
    conf = confirm("Tem certeza que deseja eliminar esse arquivo?")
    if (conf) {
      url = "http://localhost:3000/delete?file=" + file
      window.location.replace(url);
    }
  }


</script>



